{
    "builds": [{
        "src": "dictionary/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dictionary/wsgi.py"
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/" }
    ]
}